<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="default">
        <class>dp.verp.persistence.model.Report</class>
        <class>dp.verp.persistence.model.Booking</class>
        <class>dp.verp.persistence.model.Resource</class>
        <class>dp.verp.persistence.model.Session</class>
        <class>dp.verp.persistence.model.Slot</class>
        <class>dp.verp.persistence.model.User</class>
        <class>dp.verp.persistence.model.external.Lang</class>
        <class>dp.verp.persistence.model.external.I18nFacet</class>

        <properties>
            <property name="eclipselink.logging.level" value="SEVERE"/>
        </properties>
    </persistence-unit>
</persistence>